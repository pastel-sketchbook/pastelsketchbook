    "check": "biome check .",
    "check:fix": "biome check --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check:all": "bun run format:check && bun run lint && bun run test:coverage",
    "deploy:vercel": "vercel --prod --yes"
