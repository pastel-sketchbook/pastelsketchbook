{
  "name": "pastel-sketchbook",
  "private": true,
  "version": "0.2.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "bun scripts/sync-videos.ts",
    "build": "vite build",
    "preview": "vite preview",
    "sync:videos": "bun scripts/sync-videos.ts",
    "router:generate": "tsr generate",
    "format": "biome format --write .",
